drop sequence diary_user_seq;
drop sequence diary_reminder_seq;
drop sequence diary_seq;
drop sequence diary_image_seq;

CREATE SEQUENCE diary_user_seq START WITH 1 INCREMENT BY 1;
CREATE SEQUENCE diary_reminder_seq START WITH 1 INCREMENT BY 1;
CREATE SEQUENCE diary_seq START WITH 1 INCREMENT BY 1;
CREATE SEQUENCE diary_image_seq START WITH 1 INCREMENT BY 1;

DROP TABLE diary_image CASCADE CONSTRAINTS;
DROP TABLE diary CASCADE CONSTRAINTS;
DROP TABLE diary_reminder CASCADE CONSTRAINTS;
DROP TABLE diary_users CASCADE CONSTRAINTS;

CREATE TABLE diary_users (
  user_id         INT PRIMARY KEY,
  username        VARCHAR2(255) UNIQUE,
  user_password   VARCHAR2(255),
  premium         CHAR(1) CHECK (premium IN ('Y', 'N'))
);

CREATE TABLE diary_reminder (
  reminder_id       INT PRIMARY KEY,
  user_id           INT REFERENCES diary_users(user_id) ON DELETE CASCADE,
  reminder_interval INTERVAL DAY TO SECOND,
  reminder_time     DATE,
  reminder_title    VARCHAR2(255)
);

CREATE TABLE diary (
  diary_id          INT PRIMARY KEY,
  user_id           INT REFERENCES diary_users(user_id) ON DELETE CASCADE,
  cypher_text       CLOB,
  cypher_title      VARCHAR2(255),
  create_time       DATE,
  last_update_time  DATE
);

CREATE TABLE diary_image (
  image_id       INT PRIMARY KEY,
  diary_id       INT REFERENCES diary(diary_id) ON DELETE CASCADE,
  image_path     VARCHAR2(1000),
  insertion_time DATE,
  place_in_text  INT
);


create or replace
PROCEDURE RETRIEVE_OVERDUE_REMINDERS(USER_ID INTEGER, REMINDERS OUT SYS_REFCURSOR )
AS
BEGIN
  OPEN reminders FOR 
    SELECT P.REMINDER_TITLE, P.REMINDER_TIME, P.REMINDER_ID
    FROM DIARY_REMINDER P
    WHERE P.USER_ID = user_id AND P.REMINDER_TIME < SYSDATE;
  DELETE FROM DIARY_REMINDER P WHERE P.USER_ID = user_id AND P.REMINDER_TIME < SYSDATE AND P.REMINDER_INTERVAL = INTERVAL '0' DAY;
END ;
/
CREATE OR REPLACE 
PROCEDURE RETRIEVE_REMINDERS(USER_ID INTEGER, REMINDERS OUT SYS_REFCURSOR )
AS
BEGIN
  OPEN reminders FOR 
    SELECT P.REMINDER_TITLE, P.REMINDER_TIME, P.REMINDER_INTERVAL, P.REMINDER_ID
    FROM DIARY_REMINDER P
    WHERE P.USER_ID = user_id;
END;
/
CREATE OR REPLACE 
PROCEDURE DELETE_REMINDERS(USER_ID INTEGER, REMINDER_ID INTEGER)
AS
BEGIN
  DELETE FROM DIARY_REMINDER P WHERE P.user_id = user_id AND REMINDER_ID = P.REMINDER_ID;
END;
/
CREATE OR REPLACE 
PROCEDURE UPDATE_OVERDUE_REMINDERS(USER_ID INTEGER)
AS
BEGIN 
  UPDATE DIARY_REMINDER P SET P.REMINDER_TIME = P.REMINDER_TIME + P.REMINDER_INTERVAL WHERE P.USER_ID = user_id AND P.REMINDER_TIME < SYSDATE;
END;
/
CREATE OR REPLACE 
PROCEDURE INSERT_REMINDER(USER_ID INTEGER, REMINDER_TITLE VARCHAR2, REMINDER_TIME DATE, REMINDER_INTERVAL INTERVAL DAY TO SECOND)
AS
BEGIN
  INSERT INTO diary_reminder VALUES(DIARY_REMINDER_SEQ.NEXTVAL, user_id, reminder_interval, REMINDER_TIME, REMINDER_TITLE);
END;
/
CREATE OR REPLACE
PROCEDURE CHECK_USERNAME_AND_PASSWORD(USERNAME VARCHAR2, USER_PASSWORD VARCHAR2, PREMIUM OUT CHAR, USER_ID OUT INTEGER)
AS
BEGIN
SELECT U.PREMIUM, U.USER_ID INTO PREMIUM, user_id
FROM diary_users U
WHERE USERNAME = U.USERNAME AND USER_PASSWORD = U.USER_PASSWORD;
END;
/
create or replace
PROCEDURE CREATE_USER (USERNAME VARCHAR2, USER_PASSWORD VARCHAR2, FLAG OUT CHAR)
AS
BEGIN
SELECT 'N' INTO FLAG
FROM diary_users U
WHERE U.username = username;

IF FLAG <> 'N' THEN
  FLAG := 'Y';
  INSERT INTO diary_users VALUES (DIARY_USER_SEQ.NEXTVAL, username, user_password, 'N');
END IF;
END;
/
CREATE OR REPLACE
PROCEDURE UPDATE_PASSWORD (USER_ID INTEGER, CURRENT_PASSWORD INTEGER, NEW_PASSWORD INTEGER, FLAG OUT CHAR)
AS
BEGIN
SELECT 'Y' INTO FLAG
FROM DIARY_USERS U
WHERE U.USER_ID = user_id AND U.user_password = current_password;

IF FLAG <> 'Y' THEN
  FLAG := 'N';
ELSE
  UPDATE DIARY_USERS U SET U.user_password = new_password WHERE U.user_id = user_id;
END IF;
END;
/
CREATE OR REPLACE
PROCEDURE UPDATE_PASSWORD (USER_ID INTEGER)
AS
BEGIN
  UPDATE DIARY_USERS U SET U.PREMIUM = 'Y' WHERE U.user_id = user_id;
END;
/
CREATE OR REPLACE 
PROCEDURE ADD_DIARY (USER_ID INTEGER, DIARY_ID OUT INTEGER)
AS
BEGIN
  INSERT INTO DIARY VALUES(DIARY_SEQ.NEXTVAL, user_id, '', 'NEW DIARY', SYSDATE, SYSDATE);
  DIARY_ID := DIARY_SEQ.CURRVAL;
END;
/
CREATE OR REPLACE
PROCEDURE UPDATE_TITLE_AND_TEXT(DIARY_ID INTEGER, TITLE VARCHAR, TEXT CLOB)
AS
BEGIN
  UPDATE DIARY D SET D.CYPHER_TITLE = TITLE, D.cypher_text = TEXT, D.LAST_UPDATE_TIME = SYSDATE WHERE diary_id = D.diary_id;
END;
/
CREATE OR REPLACE 
PROCEDURE RETRIEVE_DIARY (DIARY_ID INTEGER, DIARY OUT SYS_REFCURSOR)
AS
BEGIN
  OPEN DIARY FOR
    SELECT D.CYPHER_TEXT, D.CYPHER_TITLE, D.CREATE_TIME, D.LAST_UPDATE_TIME
    FROM DIARY D
    WHERE diary_id = D.DIARY_ID;
END;
/
CREATE OR REPLACE 
PROCEDURE retrieve_titles (USER_ID INTEGER, DIARY OUT SYS_REFCURSOR)
AS
BEGIN
  OPEN DIARY FOR
    SELECT D.DIARY_ID, D.CYPHER_TITLE, D.CREATE_TIME, D.LAST_UPDATE_TIME
    FROM DIARY D
    WHERE user_id = D.USER_ID;
END;
/
CREATE OR REPLACE
PROCEDURE RETRIEVE_IMAGE (DIARY_ID INTEGER, IMAGES OUT SYS_REFCURSOR)
AS
BEGIN
  OPEN IMAGES FOR
    SELECT IMAGE_PATH, PLACE_IN_TEXT
    FROM DIARY_IMAGE I
    WHERE diary_id = I.DIARY_ID;
END;
/
CREATE OR REPLACE
PROCEDURE DELETE_DIARY (DIARY_ID INTEGER)
AS
BEGIN
  DELETE FROM DIARY I WHERE I.diary_id = DIARY_ID;
END;
/
create or replace
PROCEDURE DELETE_IMAGE (DIARY_ID INTEGER, IMAGE_ID INTEGER)
AS
BEGIN
  DELETE FROM DIARY_IMAGE I WHERE I.diary_id = DIARY_ID AND I.image_id = IMAGE_ID;
END;
/
create or replace
PROCEDURE INSERT_IMAGE (DIARY_ID INTEGER, PLACE_IN_TEXT INTEGER, IMAGE_PATH VARCHAR2)
AS
BEGIN
  INSERT INTO DIARY_IMAGE I VALUES (diary_image_seq.NEXTVAL, diary_id, image_path, SYSDATE, place_in_text);
END;
/

